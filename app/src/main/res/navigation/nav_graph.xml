<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainActivity">
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.to_doapp.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/action_mainActivity_to_tasksListActivity"
            app:destination="@id/tasksListActivity" />
    </activity>
    <activity
        android:id="@+id/tasksListActivity"
        android:name="com.example.to_doapp.TasksListActivity"
        android:label="activity_tasks_list"
        tools:layout="@layout/activity_tasks_list" />
</navigation>